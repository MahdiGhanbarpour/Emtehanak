                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardViewPopularExamsTeacherMain"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_marginTop="20dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="0dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewItemsTeacherMain">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:text="@string/popular_exams"
                            android:textColor="@color/black"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/ivMorePopularExamsTeacherMain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="12dp"
                            android:background="?selectableItemBackgroundBorderless"
                            android:clickable="true"
                            android:contentDescription="@string/see_more"
                            android:focusable="true"
                            android:padding="4dp"
                            android:src="@drawable/ic_arrow"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </com.google.android.material.card.MaterialCardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerPopularExamsTeacherMain"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:nestedScrollingEnabled="true"
                    android:overScrollMode="never"
                    android:paddingEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewPopularExamsTeacherMain"
                    tools:itemCount="1"
                    tools:listitem="@layout/item_popular_exam" />

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/animationViewPopularExamsTeacherMain"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:elevation="16dp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toTopOf="@+id/cardViewExperiencedTeachersTeacherMain"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewPopularExamsTeacherMain"
                    app:lottie_autoPlay="false"
                    app:lottie_colorFilter="@color/teacher_color"
                    app:lottie_loop="true"
                    app:lottie_rawRes="@raw/loading" />

                <ImageView
                    android:id="@+id/ivErrorPopularExamsTeacherMain"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:contentDescription="@string/error_communicating_server"
                    android:elevation="16dp"
                    android:src="@drawable/ic_internet_error"
                    android:visibility="gone"
                    app:layout_constraintBottom_toTopOf="@id/cardViewExperiencedTeachersTeacherMain"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewPopularExamsTeacherMain"
                    app:tint="@color/teacher_color" />


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cardViewExperiencedTeachersTeacherMain"
                    android:layout_width="match_parent"
                    android:layout_height="36dp"
                    android:layout_marginHorizontal="16dp"
                    android:layout_marginTop="228dp"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="0dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewPopularExamsTeacherMain">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:text="@string/experienced_teachers"
                            android:textColor="@color/black"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/ivMoreExperiencedTeachersTeacherMain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="12dp"
                            android:background="?selectableItemBackgroundBorderless"
                            android:clickable="true"
                            android:contentDescription="@string/see_more"
                            android:focusable="true"
                            android:padding="4dp"
                            android:src="@drawable/ic_arrow"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </com.google.android.material.card.MaterialCardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerExperiencedTeachersTeacherMain"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:nestedScrollingEnabled="true"
                    android:overScrollMode="never"
                    android:paddingEnd="8dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewExperiencedTeachersTeacherMain"
                    tools:itemCount="1"
                    tools:listitem="@layout/item_experienced_teachers" />

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@+id/animationViewExperiencedTeachersTeacherMain"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:elevation="16dp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewExperiencedTeachersTeacherMain"
                    app:lottie_autoPlay="false"
                    app:lottie_colorFilter="@color/teacher_color"
                    app:lottie_loop="true"
                    app:lottie_rawRes="@raw/loading" />

                <ImageView
                    android:id="@+id/ivErrorExperiencedTeachersTeacherMain"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:contentDescription="@string/error_communicating_server"
                    android:elevation="16dp"
                    android:src="@drawable/ic_internet_error"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardViewExperiencedTeachersTeacherMain"
                    app:tint="@color/teacher_color" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </ScrollView>

    </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>

</androidx.constraintlayout.widget.ConstraintLayout>